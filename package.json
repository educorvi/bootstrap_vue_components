{
  "name": "bootstrap_components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm-run-all -p build:*",
    "build:bundle": "vue-cli-service build --target lib --dest dist/bundle --name hello-world src/components/index.js",
    "build:hello-world": "vue-cli-service build --target lib --dest dist/hello_world --name hello-world src/components/HelloWorld/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap": "^5.0.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "document-register-element": "^1.14.10",
    "vue": "^2.6.12",
    "vue-custom-element": "^3.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
